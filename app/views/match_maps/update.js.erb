<% if params[:match_map][:results_updated_token].present? %>
  <% if @match_map.match.can_be_edited_by? current_user and !@match_map.errors.any? %>
    $('#<%= params[:match_map][:match_entries_token] %>').html("<%= escape_javascript(render('matches/match_entries_edit', :match_map => @match_map, :team => @team)) %>");
  <% end %>
  $('#<%= params[:match_map][:results_updated_token] %>').html("<%= escape_javascript(render('update_results', :match_map => @match_map)) %>");
<% elsif params[:match_map][:picture_updated_token].present? %>
  $('#<%= params[:match_map][:picture_updated_token] %>').html("<%= escape_javascript(render('update_results', :match_map => @match_map)) %>");
<% end %>
   